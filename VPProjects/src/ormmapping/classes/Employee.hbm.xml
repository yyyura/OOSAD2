<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="classes.Employee" table="employee" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="identity">
			</generator>
		</id>
		<property name="name" column="name" type="string" length="100" not-null="false" lazy="false"/>
		<property name="address" column="address" type="string" length="100" not-null="false" lazy="false"/>
		<property name="pps" column="pps" type="string" length="10" not-null="false" lazy="false"/>
		<property name="mobile" column="mobile" type="string" length="15" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
